<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grosso.retrofit2tutorial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GetPostsActivity" />
        <activity android:name=".PostUserActivity" />
        <activity android:name=".GetPostsByUserIdActivity" />
        <activity android:name=".GetPostsByMultipleParametersWithTheSameName" />
        <activity android:name=".GetPostsByUserIdAndPostIdNull" />
        <activity android:name=".GetPostsByParamsActivity" />
        <activity android:name=".GetPostByIdActivity" />
        <activity android:name=".GetIpActivity" />
        <activity android:name=".UploadFileActivity" />
        <activity android:name=".MultipartTaskActivity" />
        <activity android:name=".PostPostActivity" />
        <activity android:name=".HeaderActivity"></activity>
        <activity android:name=".HeaderDynamicActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".XMLConverterActivity"/>
        <activity android:name=".JsonConverterActivity" />
        <activity android:name=".OkHttpActivity"/>
        <activity android:name=".MainActivity" />
    </application>

</manifest>